from typing import ClassVar, List, Tuple, Set

from typing import overload
import pytket._tket.circuit
from .type_helpers import json

class Architecture:
    __hash__: ClassVar[None] = ...
    @overload
    def __init__(self, connections: List[Tuple[int,int]]) -> None: ...
    @overload
    def __init__(self, connections: List[Tuple[pytket._tket.circuit.Node,pytket._tket.circuit.Node]]) -> None: ...
    @classmethod
    def from_dict(cls, arg0: json) -> Architecture: ...
    def get_adjacent_nodes(self, node: pytket._tket.circuit.Node) -> Set[pytket._tket.circuit.Node]: ...
    def get_distance(self, node_0: pytket._tket.circuit.Node, node_1: pytket._tket.circuit.Node) -> int: ...
    def to_dict(self) -> json: ...
    def valid_operation(self, uids: List[pytket._tket.circuit.Node]) -> bool: ...
    def __deepcopy__(self, arg0: dict) -> Architecture: ...
    def __eq__(self, arg0: Architecture) -> bool: ...
    @property
    def coupling(self) -> List[Tuple[pytket._tket.circuit.Node,pytket._tket.circuit.Node]]: ...
    @property
    def nodes(self) -> List[pytket._tket.circuit.Node]: ...

class FullyConnected:
    __hash__: ClassVar[None] = ...
    def __init__(self, n: int, label: str = ...) -> None: ...
    @classmethod
    def from_dict(cls, arg0: json) -> FullyConnected: ...
    def to_dict(self) -> json: ...
    def __deepcopy__(self, arg0: dict) -> FullyConnected: ...
    def __eq__(self, arg0: FullyConnected) -> bool: ...
    @property
    def nodes(self) -> List[pytket._tket.circuit.Node]: ...

class RingArch(Architecture):
    def __init__(self, nodes: int, label: str = ...) -> None: ...
    def __deepcopy__(self, arg0: dict) -> RingArch: ...

class SquareGrid(Architecture):
    @overload
    def __init__(self, n_rows: int, n_columns: int, label: str = ...) -> None: ...
    @overload
    def __init__(self, n_rows: int, n_columns: int, n_layers: int = ..., label: str = ...) -> None: ...
    def qind_to_squind(self, index: int) -> Tuple[int,int]: ...
    def squind_to_qind(self, row: int, column: int) -> int: ...
    def __deepcopy__(self, arg0: dict) -> SquareGrid: ...
